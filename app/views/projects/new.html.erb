<h1>New Project</h1>

<%= form_with scope: :drelief, url: projects_path, local: true do |form| %>
 
  
  <p>
    <%= form.label 'Project Name' %><br>
    <%= form.text_field :title %>
  </p>
 
  <p>
    <%= form.label 'Description' %><br>
    <%= form.text_area :description %>
  </p>
 
  <p>
    <%= form.submit %>
  </p>
<% end %>

<%= link_to 'Back', projects_path %>